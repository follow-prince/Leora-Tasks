{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js", 
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*\\.(json|js|css|ico))",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "use": "@vercel/static"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  